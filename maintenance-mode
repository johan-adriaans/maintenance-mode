#!/usr/bin/php
<?php

$server = new IZI_Daemon( '0.0.0.0', 80 );

$payload = <<<DATA
<!doctype html>
<html>
  <head>
    <title>Onderhoud</title>
    <meta name="robots" content="noindex">
    <style>
      html,body { height: 100%; }
      body {
        background-color: #eeeeee;
        background-repeat: no-repeat;
        background-position: 50% 25%;
        background-image: url(data:image/gif;base64,);
      }
    </style>
  </head>
  <body>
  </body>
</html>
DATA;

$server->mainLoop( $payload );

class IZI_Daemon
{
  protected static $_instance; 
  protected static $debug;
  protected $socket;
  protected $clients = array();
  protected $maxclients = 80;
  protected $timeout = 30;

  public function __construct( $address = '', $port = 8080, $timeout = 30, $maxclients = 30, $debug = true )
  {
    self::$debug = $debug;
    $this->maxclients = $maxclients;
    $this->timeout = $timeout;

    if( empty( $address ) ) {
      $addresses = explode("\n", trim( shell_exec("/sbin/ifconfig | grep 'inet addr:' | cut -d: -f2 | awk '{ print $1}'")));
      $address = reset( $addresses );
    }

    // Create a streaming socket, of type TCP/IP
    $this->socket = socket_create( AF_INET, SOCK_STREAM, SOL_TCP );
    socket_set_option( $this->socket, SOL_SOCKET, SO_REUSEADDR, 1 );
    if ( !@socket_bind( $this->socket, $address, $port ) ) {
      print "Could not start server on {$address}:{$port}. Check permissions and/or already running servers." . PHP_EOL;
      exit();
    }
    socket_listen( $this->socket );

    print date("M t H:i:s ") . "[__construct] IZI Server running on " . $address . ":" . $port . "\r\n";
  }
  
  public static function getInstance ( $address = '', $port = 8080, $timeout = 30, $maxclients = 30, $debug = true )
  {
    if( !is_object( self::$_instance ) ) {
      self::$_instance = new self( $address, $port, $timeout, $maxclients, $debug );
    }
    return self::$_instance;
  }

  public function mainLoop ( $payload )
  {   
    while( 1 ) {
      // give back some CPU cycles to avoid resource hogging when looping in idle state
      usleep(1000);

      // initialize array of information about connected customers
      $read = array( $this->socket );
      for( $i = 0; $i < $this->maxclients; $i++ ) {
        if( !isset( $this->clients[ $i ] )) continue;

        if( isset( $this->clients[ $i ]["sock"] ) && is_resource( $this->clients[ $i ]["sock"] )) {
          // if this socket is open for shorter than 30 seconds, add it to the array of connected clients
          if( time() > ( $this->clients[ $i ]["since"] + $this->timeout )) {
            // timeout has occurred and the connected client will be disconnected and removed
            if( self::$debug ) print date("M t H:i:s ") . "[mainLoop] Client with IP " . $this->clients[ $i ]["ip"] . " timed out\r\n";
            socket_close( $this->clients[ $i ]["sock"] );
            unset( $this->clients[ $i ] );
          } else {
            $read[] = $this->clients[ $i ]["sock"];
          }
        }
      }
     
      // get a list of all the clients that have data to be read from
      // if there are no clients with data, go to next iteration
      if( socket_select( $read, $write = null, $except = null, 0, 15 ) < 1 ) continue;
      
      // check if there is a client trying to connect
      if( in_array( $this->socket, $read )) {
        // accept the client, and add to the clients array
        $newsock = socket_accept( $this->socket );
        $ip = null;
        socket_getpeername( $newsock, $ip );
                
        if( self::$debug ) print date("M t H:i:s ") . "[mainLoop] Client with IP " . $ip . " connected\r\n";

        array_unshift( $this->clients, array(
          "sock" => $newsock,
          "ip" => $ip,
          "since" => time(),
          "buffer" => ""
        ));
        
        // remove the listening socket from the clients-with-data array
        $key = array_search( $this->socket, $read );
        unset( $read[ $key ] );
      }
  
      // If a client is trying to write - handle it now
      for( $i = 0; $i < $this->maxclients; $i++ ) {
        if( !isset( $this->clients[ $i ] )) continue;

        if( in_array( $this->clients[ $i ]["sock"] , $read )) {
          try {
            $input = socket_read( $this->clients[ $i ]["sock"] , 2048 );
          } catch( Exception $e ) {
            if( self::$debug ) print date("M t H:i:s ") . "[mainLoop] Client with IP " . $this->clients[ $i ]["ip"] . " disconnected\r\n";
            unset( $this->clients[ $i ] );
            continue;
          }

          if( $input === false ) {
            // Zero length string meaning disconnected
            if( self::$debug ) print date("M t H:i:s ") . "[mainLoop] Client with IP " . $this->clients[ $i ]["ip"] . " disconnected\r\n";
            unset( $this->clients[ $i ] );
            continue;
          }
          
          $this->clients[ $i ]["buffer"] .= $input;
          if( $this->clients[ $i ]["buffer"] ) {
            $matches = null;
            if( preg_match('/^(GET|POST)\s+(.+?)\s+HTTP/i', $this->clients[ $i ]["buffer"], $matches )) {
              list( $header, $content ) = explode("\r\n\r\n", $this->clients[ $i ]["buffer"] );

              $body = $payload;
             
              $header = "HTTP/1.1 200 OK\r\n";
              $header .= "Content-Length: " . strlen( $body ) . "\r\n";
              $header .= "Connection: close\r\n\r\n";
              
              $data = $header . $body;
              $start_chunk = 0;
              while( $start_chunk <= strlen( $data )) {
                $text_chunk = substr( $data, $start_chunk, 2048 );
                socket_write( $this->clients[ $i ]["sock"], $text_chunk, strlen( $text_chunk ));
                $start_chunk += 2048;
              }
            }

            if( self::$debug ) print date("M t H:i:s ") . "[mainLoop] Client with IP " . $this->clients[ $i ]["ip"] . " disconnected\r\n";
            socket_close( $this->clients[ $i ]["sock"] );
            unset( $this->clients[ $i ] );
          }          
        }
      }
    }
  }
}
?>
